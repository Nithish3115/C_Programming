// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>



int main() {
 int num,cpy,rev,rem,r;
 scanf("%d",&num);
 int mul=1;
 int sum=0;
 int k=0;
 int n=num;
 while (num!=0){
     rem=num%10;
     rev=rev*10+rem;
     num=num/10;
 }
 if(n == rev){
while(n!=0){
    r=n%10;
     mul=mul*r;
      n=n/10;
}
printf("%d",mul);}
if(n != rev){
while(n!=0){
    r=n%10;
     sum=sum+r;
      n=n/10;
}
printf("%d",sum);}

  

 
 
    return 0;
}